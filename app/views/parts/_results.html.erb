<div class="container d-flex justify-content-between align-items-center">
	<%= paginate @part_results%>
	
	<form class="form-inline">
	<%= search_form_for q, :url => {:action => action } do |f|%>
	<div class="form-group">
	<%= f.select :category_eq, Part::CATEGORIES, {include_blank: "Select Part Category"}, class: "form-control" %>
</div>


<div class="form-group ml-3">
	<%= f.submit "Find part", class: "btn btn-info"%>
</div>
<% end %>
<br \>
<%= link_to 'Back', dashboard_parts_path, class: "btn btn-info " %>
</div>